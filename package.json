{
  "name": "busy-hours",
  "version": "0.0.0",
  "description": "Hacky scraper for Google Popular Times data (Node.js)",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rak-trzustki/busy-hours.git"
  },
  "keywords": [
    "google",
    "popular times",
    "places api"
  ],
  "author": "E <e@tausendsassa.io>",
  "dependencies": {
    "@google/maps": "^0.4.5",
    "axios": "^0.17.1",
    "cheerio": "latest",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-es6-promise": "^1.1.1",
    "babel-preset-es2015": "^6.24.1"
  },
  "babel": {
    "plugins": [
      "es6-promise"
    ]
  }
}
